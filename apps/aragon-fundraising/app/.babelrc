{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "useBuiltIns": "entry",
        "corejs": 3,
        "shippedProposals": true,
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/plugin-proposal-class-properties", { "loose": true }, 'class-properties'],
    ["@babel/plugin-proposal-optional-chaining", { "loose": true }, 'optional-chaining'],
    ["@babel/plugin-proposal-nullish-coalescing-operator", { "loose": true }, 'nullish-coalescing-operator'],
    ["@babel/plugin-proposal-private-methods", { "loose": true }, 'private-methods'],
    ["styled-components", { "displayName": true }]
  ],
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": "commonjs",
            "targets": { "node": "current" }
          }
        ]
      ]
    }
  }
}
